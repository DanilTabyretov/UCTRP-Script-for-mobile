task.wait(1)
local AdminEvent = game:GetService("ReplicatedStorage"):WaitForChild("$ArCommunicator")
local plr = game:GetService("Players").LocalPlayer

function firetouchinterest(Entity, Toucher)
	local Old_Pos = Toucher.CFrame

    Toucher.CFrame = Entity.CFrame * CFrame.new(0,2,0)

    task.wait(0.05)

    Toucher.CFrame = Old_Pos
end

local Gui = Instance.new("ScreenGui", plr.PlayerGui)
Gui.ResetOnSpawn = false
Gui.IgnoreGuiInset = true
script.Parent = Gui

local MainFrame = Instance.new("Frame", Gui)
MainFrame.BackgroundColor3 = Color3.new(0,0,0)
MainFrame.BackgroundTransparency = 0.25
