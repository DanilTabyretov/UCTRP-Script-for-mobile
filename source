local CurrentMorph = nil
local MorphIsRig = false

local SearchMorphBox = Instance.new("TextBox", Page1)
SearchMorphBox.BackgroundColor3 = Color3.fromRGB(86,86,86)
SearchMorphBox.Position = UDim2.new(0.041,0,-0.092,0)
